{
  "name": "tampermonkey-scripts",
  "version": "1.0.0",
  "description": "TampermonkeyScripts",
  "scripts": {
    "serve": "webpack --mode=development --progress --watch",
    "build": "webpack --mode=production --progress",
    "deploy": "npm run build && gh-pages -d ./dist",
    "lint": "eslint --fix --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sakura-flutter/tampermonkey-scripts.git"
  },
  "keywords": [
    "tampermonkey"
  ],
  "author": "sakura-flutter",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/sakura-flutter/tampermonkey-scripts/issues"
  },
  "homepage": "https://github.com/sakura-flutter/tampermonkey-scripts#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@vue/babel-plugin-jsx": "^1.0.0-rc.3",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^15.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "postcss-loader": "^4.0.4",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.2",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "npm run lint"
    ]
  }
}
